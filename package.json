{
  "version": "1.0.1",
  "license": "MIT",
  "repository": "jonyonson/only-truthy",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "np"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "only-truthy",
  "author": {
    "name": "Jonathan Taylor",
    "email": "jonathan.taylor.dev@gmail.com",
    "url": "https://jonathantaylor.io"
  },
  "keywords": [
    "falsy",
    "truthy",
    "object",
    "filter"
  ],
  "module": "dist/only-truthy.esm.js",
  "size-limit": [
    {
      "path": "dist/only-truthy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/only-truthy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "np": "^7.6.0",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
